.content
  max-width 700px
  margin    80px auto
  img
    display    block
    text-align center
    max-width  100%
    margin     20px 0
  h1
    font-size     40px
    text-align    center
    font-weight   bold
    border-bottom 1px solid #ccc
    margin        60px 0 30px
  h2
    font-size   30px
    font-weight bold
    margin      60px 0 30px
  li
    list-style-type square
    margin          20px 0 0 40px
  table
    width 90%
    margin 20px 5% 50px
    td
      border-bottom 1px solid #ccc
      padding 17px
